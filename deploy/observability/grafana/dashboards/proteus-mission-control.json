{
  "uid": "proteus-mission-control",
  "title": "Proteus Mission Control",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Success Rate by Domain",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "sum by(domain) (rate(proteus_jobs_total{state=\"succeeded\"}[5m])) / sum by(domain) (rate(proteus_jobs_total[5m]))",
          "legendFormat": "{{domain}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Ban Spikes (403/429)",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      },
      "targets": [
        {
          "expr": "sum by(domain) (rate(proteus_failures_total{reason=~\"http_403|http_429\"}[5m]))",
          "legendFormat": "{{domain}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "LLM Token Rate by Tenant",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by(tenant) (rate(proteus_llm_tokens_total[5m]))",
          "legendFormat": "{{tenant}}",
          "refId": "A"
        }
      ]
    },
    {
      "id": 4,
      "type": "bargauge",
      "title": "Queue Depth",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "max by(queue) (proteus_queue_depth)",
          "legendFormat": "{{queue}}",
          "refId": "A"
        }
      ],
      "options": {
        "displayMode": "basic",
        "orientation": "horizontal"
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Proxy Errors by Provider",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by(provider) (rate(proteus_proxy_errors_total[5m]))",
          "legendFormat": "{{provider}}",
          "refId": "A"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right"
        }
      }
    }
  ]
}
